    {
      "id": "AztTIVODpdMN53HEnGm0e",
      "type": "text",
      "x": 428.39111328125,
      "y": 199.66876220703125,
      "width": 704,
      "height": 78,
      "angle": 0,
      "strokeColor": "#000000",
      "backgroundColor": "transparent",
      "fillStyle": "hachure",
      "strokeWidth": 1,
