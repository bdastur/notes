<MyButton@Button>:
    color: .8, .9, 0, 1
    background_color: 0.1, 0.2, 0.2, 1
    font_size: 48
    size: 400, 90
    border: 30, 30, 30, 30

<MyWidget>:
    MyButton:
        text: "Widget Button 1"
        pos: root.x + 200, root.top - 400
    MyButton:
        text: "Widget Button 2"
        pos: root.right - 600, root.y + 100


